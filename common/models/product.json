{
  "name": "Product",
  "plural": "Products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "availableQuantity": {
      "type": "number",
      "required": true
    },
    "warranty": {
      "type": "string",
      "required": true
    },
    "cityName": {
      "type": "string"
    },
    "publishDate": {
      "type": "date",
      "required": true
    },
    "availability": {
      "type": "boolean",
      "required": true
    },
    "measures": {
      "type": "object",
      "required": true,
      "default": {
        "width": 0,
        "heigth": 0,
        "weigth": 0,
        "large": 0
      }
    },
    "city_id": {
      "type": "number",
      "required": true
    },
    "user_id": {
      "type": "number",
      "required": true
    },
    "condition": {
      "type": "string",
      "required": true,
      "default": "Used"
    },
    "location": {
      "type": "object",
      "required": true,
      "default": {
        "latitude": 0,
        "longitude": 0
      }
    },
    "photos": {
      "type": ["object"],
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "categories": {
      "type": "hasMany",
      "model": "Category",
      "foreignKey": "product_id",
      "options": {
        "nestRemoting": true
      },
      "through": "ProductXCategory"
    },
    "rappiUser": {
      "type": "belongsTo",
      "model": "RappiUser",
      "foreignKey": "user_id"
    },
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "city_id"
    }
  },
  "acls": [],
  "methods": {}
}
