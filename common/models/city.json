{
  "name": "City",
  "plural": "Cities",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "country_id": {
      "type": "number",
      "required": true
    },
    "latitude": {
      "type": "string",
      "required": true
    },
    "longitude": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "country": {
      "type": "belongsTo",
      "model": "Country",
      "foreignKey": "country_id"
    },
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": "city_id"
    }
  },
  "acls": [],
  "methods": {}
}
